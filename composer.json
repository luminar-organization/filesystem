{
  "name": "luminar-organization/filesystem",
  "description": "A File System component for the Luminar framework.",
  "type": "library",
  "require": {
    "php": ">=8.2",
    "ext-fileinfo": "*",
    "ext-openssl": "*",
    "ext-zlib": "*",
    "ext-zip": "*",
    "ext-bz2": "*"
  },
  "autoload": {
    "psr-4": {
      "Luminar\\FileSystem\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Luminar\\FileSystem\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=11.3.1"
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev"
}
